<?xml version="1.0" encoding="utf-8"?>
        <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:cardUseCompatPadding="true"
            android:id="@+id/card_person"
            android:layout_margin="5dp"
            app:cardCornerRadius="8dp"
            app:cardBackgroundColor="@color/card_bg">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="အကျိုးခံစားခွင့်ရှိသူ"
                    android:id="@+id/lblbeneficiary_listiem"
                    android:textColor="@color/cardview_light_background"
                    android:layout_weight="1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    />
                <EditText
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lblbeneficiary_listiem"
                    android:hint="ဖြည့်စွက်ပါ"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="5dp"
                    android:id="@+id/edt_beneficiaryname_listiem"
                    android:singleLine="true"
                    android:background="@drawable/customer_bg"
                    android:paddingLeft="14dp"
                    android:inputType="textNoSuggestions"
                    android:digits="abcdefghijklmnopqrstuvwxyz ABCDEFGHIJKLMNOPQRSTUVWXYZ"
                    />

                <TextView
                    android:id="@+id/lblnrc_listiem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:text="အမျိုးသားမှတ်ပုံတင်အမှတ်"
                    android:textColor="@color/cardview_light_background"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_beneficiaryname_listiem" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/lblnrc_listiem"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    android:id="@+id/linearnrc">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/customer_bg"
                        android:padding="3dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="5dp">
                        <Spinner
                            android:id="@+id/spn_fnrc_listiem"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:dropDownWidth="80dp" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/customer_bg"
                        android:padding="3dp"
                        android:layout_marginTop="5dp">
                        <Spinner
                            android:id="@+id/spn_snrc_listiem"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:dropDownWidth="100dp"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/linearnrc"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    android:id="@+id/linear_snrc">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@drawable/customer_bg"
                        android:padding="3dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="5dp">
                        <Spinner
                            android:id="@+id/spn_tnrc_listiem"
                            android:layout_width="match_parent"
                            android:layout_height="45dp"
                            android:dropDownWidth="80dp"/>
                    </RelativeLayout>
                    <EditText
                        android:layout_weight="1"
                        android:id="@+id/tie_nrcno_listiem"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:imeOptions="actionGo"
                        android:singleLine="true"
                        android:inputType="number"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginTop="5dp"
                        android:background="@drawable/customer_bg"
                        android:hint="123456"
                        android:paddingLeft="14dp"
                        android:paddingTop="3dp"
                        app:layout_constraintTop_toBottomOf="@+id/linearnrc"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/lblfathername_listiem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="အဘအမည်"
                    android:textColor="@color/cardview_light_background"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/linear_snrc"/>
                <EditText
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/lblfathername_listiem"
                    android:hint="ဖြည့်စွက်ပါ"
                    android:paddingLeft="14dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:id="@+id/edt_beneficiaryfathername_listiem"
                    android:singleLine="true"
                    android:imeOptions="actionGo"
                    android:background="@drawable/customer_bg"
                    android:inputType="textNoSuggestions"
                    android:digits="abcdefghijklmnopqrstuvwxyz ABCDEFGHIJKLMNOPQRSTUVWXYZ"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/edt_beneficiaryfathername_listiem"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    android:id="@+id/linear_lblrap">
                    <TextView
                        android:layout_weight="1"
                        android:id="@+id/lblrelationship_listiem"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:textAlignment="center"
                        android:text="တော်စပ်ပုံ"
                        android:textColor="@color/cardview_light_background"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linear_snrc" />
                    <TextView
                        android:id="@+id/lblage_listiem"
                        android:layout_weight="1.3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:textAlignment="center"
                        android:text="အသက်"
                        android:textColor="@color/cardview_light_background"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/relative_beneficiaryrelationship" />
                    <TextView
                        android:id="@+id/lblpercentage_listiem"
                        android:layout_weight="1.3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:textAlignment="center"
                        android:text="ရာခိုင်နှုန်း"
                        android:textColor="@color/cardview_light_background"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/edt_beneficiaryage" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/linear_lblrap"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp"
                    android:id="@+id/linear_spnrap">
                    <RelativeLayout
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/lblrelationship"
                        android:hint="@string/lblbeneficiaryrelation"
                        android:layout_marginTop="5dp"
                        android:id="@+id/relative_beneficiaryrelationship"
                        android:background="@drawable/customer_bg">
                        <Spinner
                            android:id="@+id/spn_relationship_listiem"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>
                    </RelativeLayout>

                    <EditText
                        android:layout_weight="1.3"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/lblage"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="5dp"
                        android:id="@+id/edt_beneficiaryage_listiem"
                        android:singleLine="true"
                        android:imeOptions="actionGo"
                        android:inputType="number"
                        android:background="@drawable/customer_bg"
                        android:paddingLeft="10dp"/>

                    <EditText
                        android:layout_weight="1.3"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/lblpercentage"
                        android:id="@+id/edt_beneficiarypercent_listiem"
                        android:singleLine="true"
                        android:layout_marginTop="5dp"
                        android:inputType="numberDecimal"
                        android:background="@drawable/customer_bg"
                        android:paddingLeft="10dp"/>
                </LinearLayout>


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/btndelete_listiem"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linear_spnrap"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/roundedbutton_bg"
                    android:padding="5dp"
                    android:src="@drawable/btn_delete"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>
